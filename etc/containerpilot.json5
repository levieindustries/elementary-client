{
  consul: "{{ .CONSUL_URL }}:8500",
  jobs: [
    {
      name: "elementary-client",
      exec: "nginx",
      port: 80,
      tags: ["consul-lb.url=elementary.com.dev"],
      health: {exec: "curl -fsSo /dev/null localhost", interval: 5, ttl: 10},
      consul: {deregisterCriticalServiceAfter: '10m'}
    },
  ],
}
